diff --git a/src/sndfile.c b/src/sndfile.c
index 020e0987..ccc6018b 100644
--- a/src/sndfile.c
+++ b/src/sndfile.c
@@ -3126,6 +3126,20 @@ psf_open_file (SF_PRIVATE *psf, SF_INFO *sfinfo)
 			psf->sf.format = format_from_extension (psf) ;
 		} ;
 
+#ifdef MAGMA_ENABLE_CANARIES
+	MAGMA_LOG("w64_rf64_wav_wavx",
+    MAGMA_OR(
+        MAGMA_OR(
+            SF_CONTAINER(psf->sf.format) == SF_FORMAT_W64,
+            SF_CONTAINER(psf->sf.format) == SF_FORMAT_RF64
+        ),
+        MAGMA_OR(
+            SF_CONTAINER(psf->sf.format) == SF_FORMAT_WAV,
+            SF_CONTAINER(psf->sf.format) == SF_FORMAT_WAVEX
+        )
+    )
+);
+#endif
 	/* Prevent unnecessary seeks */
 	psf->last_op = psf->file.mode ;
 
diff --git a/src/wavlike.c b/src/wavlike.c
index 69b8d352..ec06b0ab 100644
--- a/src/wavlike.c
+++ b/src/wavlike.c
@@ -143,6 +143,9 @@ wavlike_read_fmt_chunk (SF_PRIVATE *psf, int fmtsize)
 					&(wav_fmt->min.samplerate), &(wav_fmt->min.bytespersec),
 					&(wav_fmt->min.blockalign), &(wav_fmt->min.bitwidth)) ;
 
+#ifdef MAGMA_ENABLE_CANARIES
+	MAGMA_LOG("WAVE_FORMAT_EXTENSIBLE", wav_fmt->format == WAVE_FORMAT_EXTENSIBLE);
+#endif
