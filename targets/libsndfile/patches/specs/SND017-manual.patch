diff --git a/src/sndfile.c b/src/sndfile.c
index 365c1361..3473513f 100644
--- a/src/sndfile.c
+++ b/src/sndfile.c
@@ -3114,6 +3114,17 @@ psf_open_file (SF_PRIVATE *psf, SF_INFO *sfinfo)
 			psf->sf.format = format_from_extension (psf) ;
 		} ;
 
+#ifdef MAGMA_ENABLE_CANARIES
+	if (SF_CONTAINER (psf->sf.format) == SF_FORMAT_W64) {
+		MAGMA_LOG("w64", 1);
+	}
+	if (SF_CONTAINER (psf->sf.format) == SF_FORMAT_RF64) {
+		MAGMA_LOG("rf64", 1);
+	}
+	if (SF_CONTAINER (psf->sf.format) == SF_FORMAT_WAV || SF_CONTAINER (psf->sf.format) == SF_FORMAT_WAVEX) {
+		MAGMA_LOG("wav", 1);
+	}
+#endif
 	/* Prevent unnecessary seeks */
 	psf->last_op = psf->file.mode ;
 
diff --git a/src/wavlike.c b/src/wavlike.c
index 3c206b40..77ea8069 100644
--- a/src/wavlike.c
+++ b/src/wavlike.c
@@ -143,6 +143,11 @@ wavlike_read_fmt_chunk (SF_PRIVATE *psf, int fmtsize)
 					&(wav_fmt->min.samplerate), &(wav_fmt->min.bytespersec),
 					&(wav_fmt->min.blockalign), &(wav_fmt->min.bitwidth)) ;
 
+#ifdef MAGMA_ENABLE_CANARIES
+	if (wav_fmt->format == WAVE_FORMAT_EXTENSIBLE) {
+		MAGMA_LOG("WAVE_FORMAT_EXTENSIBLE", 1);
+	}
+#endif
 	psf_log_printf (psf, "  Format        : 0x%X => %s\n", wav_fmt->format, wavlike_format_str (wav_fmt->format)) ;
 	psf_log_printf (psf, "  Channels      : %d\n", wav_fmt->min.channels) ;
 	psf_log_printf (psf, "  Sample Rate   : %d\n", wav_fmt->min.samplerate) ;
